{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug CSPBench - Main",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/.venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "PYTHONDONTWRITEBYTECODE": "1",
                "OMP_NUM_THREADS": "1",
                "MKL_NUM_THREADS": "1",
                "NUMEXPR_NUM_THREADS": "1",
                "OPENBLAS_NUM_THREADS": "1"
            },
            "args": [],
            "justMyCode": false,
            "stopOnEntry": false,
            "showReturnValue": true,
            "subProcess": true
        },
        {
            "name": "Debug CSPBench - Web",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/.venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "PYTHONDONTWRITEBYTECODE": "1",
                "DEBUG": "true"
            },
            "args": [
                "web"
            ],
            "justMyCode": false,
            "stopOnEntry": false,
            "showReturnValue": true,
            "subProcess": true
        },
        {
            "name": "Debug CSPBench - TEMPLATE",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/.venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "PYTHONDONTWRITEBYTECODE": "1",
                "OMP_NUM_THREADS": "1",
                "MKL_NUM_THREADS": "1",
                "NUMEXPR_NUM_THREADS": "1",
                "OPENBLAS_NUM_THREADS": "1"
            },
            "args": ["batches/TEMPLATE.yaml"],
            "justMyCode": false,
            "stopOnEntry": false,
            "showReturnValue": true,
            "subProcess": true
        },
        {
            "name": "Debug CSPBench - Dataset Save",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/.venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "PYTHONDONTWRITEBYTECODE": "1",
                "OMP_NUM_THREADS": "1",
                "MKL_NUM_THREADS": "1",
                "NUMEXPR_NUM_THREADS": "1",
                "OPENBLAS_NUM_THREADS": "1"
            },
            "args": [
                "datasetsave"
            ],
            "justMyCode": false,
            "stopOnEntry": false,
            "showReturnValue": true,
            "subProcess": true
        },
        {
            "name": "Debug CSPBench - Work Restart (Dynamic ID)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/.venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "PYTHONDONTWRITEBYTECODE": "1",
                "OMP_NUM_THREADS": "1",
                "MKL_NUM_THREADS": "1",
                "NUMEXPR_NUM_THREADS": "1",
                "OPENBLAS_NUM_THREADS": "1"
            },
            "args": [
                "work",
                "restart",
                "${input:workId}"
            ],
            "justMyCode": false,
            "stopOnEntry": false,
            "showReturnValue": true,
            "subProcess": true
        }
    ],
    "inputs": [
        {
            "id": "workId",
            "description": "Digite o ID do trabalho para reiniciar:",
            "default": "",
            "type": "promptString"
        }
    ]
}