# Environment variables for CSPBench
# Copy this file to .env and modify the values as needed

# NCBI (National Center for Biotechnology Information) API access
NCBI_EMAIL=your.email@example.com
NCBI_API_KEY=your_ncbi_api_key_here

# Web Interface Configuration  
WEB_HOST=0.0.0.0
WEB_PORT=8000
WEB_RELOAD=true

# System overrides (optional)
EXECUTOR_IMPL=Executor
EXPORT_FMT=json

# Application Settings
PYTHONPATH=/app
LOG_LEVEL=INFO

# Docker Configuration
COMPOSE_PROJECT_NAME=cspbench-web
DATASET_PATH=./datasets

# Web interface (optional)
WEB_HOST=0.0.0.0
WEB_PORT=8000
